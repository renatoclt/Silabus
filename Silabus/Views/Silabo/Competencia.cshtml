
@{
    ViewBag.Title = "Competencia";
    Layout = "~/Views/Shared/_LayoutSilabo.cshtml";
}


@if (ViewBag.Competencia.Estado == 1)
{
    <ul>
        @foreach (var competencia in ViewBag.Competencia.Competencias)
        {
            <li >@competencia.Descripcion</li>
        }
    </ul>


}
else
{
    <ul>
        @foreach (var competencia in ViewBag.Competencia.Competencias)
        {
        <li >@competencia.Descripcion
        <select>
            @foreach (var fase in ViewBag.Fases)
            {
                <option>@fase.Descripcion</option>
            }
        </select>
        </li>
        }
    </ul>
}
