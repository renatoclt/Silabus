@model IEnumerable<Silabus.Models.SilaboDocente>

@{
    Layout = null;
    Layout = "~/Views/Shared/_LayoutSilabo.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Silabo.Asignaturas.Codigo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Silabo.Asignaturas.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Silabo.Sumilla)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Silabo.Asignaturas.Codigo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Silabo.Asignaturas.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Silabo.Sumilla)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id= item.Silabo.Id }) 
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
